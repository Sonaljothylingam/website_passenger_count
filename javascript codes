let countEl=document.getElementById("count-el")
let saveEl=document.getElementById("save-el")

let count =0
//console.log(saveEl)
function increment(){
   count+=1
   countEl.textContent=count
   
}
function save(){
 let ctt = count+" - "
  saveEl.textContent+=ctt
    console.log(count)
    count=0
    countEl.textContent=count
}   
